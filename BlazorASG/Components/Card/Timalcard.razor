@page "/TemlatIcon"
<section class="pt-0" style="margin-top:3%">
	<!-- Title and content -->

	
	@if (CardTextAndIconlsList != null)
	{
		<TemlatIcon Params="CardTextAndIconlsList" />
	}
	<!-- Image -->
	<div class="container mt-n7 mt-sm-n9">
