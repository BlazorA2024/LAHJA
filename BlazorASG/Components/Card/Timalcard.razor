				</svg>
			</span>

			<!-- Images -->

			<div class="row g-4 g-lg-5 rounded-4 " style="padding:5%">

				<!-- Feature item -->
				@foreach (var cardDetai in cardDetailsL)
				{
					<TemlatIconIsItms Params="@cardDetai" />
				}

				<!-- Feature item -->
				

			</div>
			<!-- Decoration image -->
			<img src="/chat-heart.png" class="position-absolute top-0 start-0 ms-8 mt-n4 d-none d-sm-block" alt="">
		</div>
	</div>
</section>

