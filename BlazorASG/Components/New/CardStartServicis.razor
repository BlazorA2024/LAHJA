    private async Task StartSpeechRecognition()
    {
        try
        {
            // استدعاء JavaScript لبدء التحدث
            await JSRuntime.InvokeVoidAsync("initializeRecognition");
            await JSRuntime.InvokeVoidAsync("startRecognition");
        }
        catch (Exception ex)
        {
            Console.WriteLine("Error starting speech recognition: " + ex.Message);
        }
    }

    // إيقاف التعرف على الصوت
    private async Task StopSpeechRecognition()
    {
        try
        {
            // استدعاء JavaScript لإيقاف التحدث
            await JSRuntime.InvokeVoidAsync("stopRecognition");
        }
        catch (Exception ex)
        {
            Console.WriteLine("Error stopping speech recognition: " + ex.Message);
        }
    }

    // استقبال النص المحول من JavaScript
    [JSInvokable]
    public void ReceiveSpeechResult(string transcript)
    {
        // تحديث النص المحول في Blazor
        recognizedText = transcript;
        StateHasChanged(); // لتحديث واجهة المستخدم
    }
