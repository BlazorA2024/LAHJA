<div class="pt-140 pb-140 overflow-hidden">

        <div class="container">
            <h3 class="page-title">غرفة ممارسة اللهجة</h3>

            <!-- علامات التبويب -->
            <div class="tabs">
                <button class="tab-btn active" onclick="showTab('chat')">غرفة دردشة</button>
                <button class="tab-btn" onclick="showTab('ai')">حوار مع الذكاء الاصطناعي</button>
            </div>

            <!-- شبكة المحتوى -->
            <div class="grid-container">
