                    <p class="comparison-result">@comparisonResult</p>
                </section>
            </div>
        </div>
    </div>



    <script>
         function showTab(tabId) {
        // إخفاء جميع الأقسام
        const sections = document.querySelectorAll('.tab-section');
        sections.forEach(section => section.classList.remove('active'));

        // إلغاء تحديد جميع الأزرار
        const buttons = document.querySelectorAll('.tab-btn');
        buttons.forEach(btn => btn.classList.remove('active'));

        // إظهار القسم المحدد
        document.getElementById(tabId).classList.add('active');
        // تمييز الزر النشط
        document.querySelector(`.tab-btn[onclick="showTab('${tabId}')"]`).classList.add('active');
    }


        </script>
<script>
    let mediaRecorder;
    let audioChunks = [];

    window.startRecording = async () => {
        const stream = await navigator.mediaDevices.getUserMedia({ audio: true });
        mediaRecorder = new MediaRecorder(stream);
        mediaRecorder.ondataavailable = (event) => {
            audioChunks.push(event.data);
        };
        mediaRecorder.start();
    };

    window.stopRecording = async () => {
        mediaRecorder.stop();
        return new Promise((resolve) => {
            mediaRecorder.onstop = () => {
                const audioBlob = new Blob(audioChunks, { type: 'audio/wav' });
                audioChunks = [];
                const audioUrl = URL.createObjectURL(audioBlob);
                resolve(audioUrl);
            };
        });
    };
</script>

@code {
    private string messageInput = string.Empty;
    private List<string> messages = new();
    private HubConnection? chatHub;

    protected override async Task OnInitializedAsync()
    {
        chatHub = new HubConnectionBuilder()
            .WithUrl(NavigationManager.ToAbsoluteUri("/chathub"))
            .Build();

        chatHub.On<string>("ReceiveMessage", (message) =>
        {
            messages.Add(message);
            InvokeAsync(StateHasChanged);
        });

        await chatHub.StartAsync();
    }

    private async Task SendMessage()
    {
        if (!string.IsNullOrWhiteSpace(messageInput))
        {
            await chatHub!.SendAsync("SendMessage", messageInput);
            messageInput = string.Empty;
        }
    }

    private string aiInput = string.Empty;
    private string aiResponse = string.Empty;

    private async Task InteractWithAI()
    {
        aiResponse = $"رد الذكاء الاصطناعي: {aiInput}";
        aiInput = string.Empty;
    }

    private string comparisonResult = string.Empty;

    private async Task StartRecording()
    {
        await JSRuntime.InvokeVoidAsync("startRecording");
    }

    private async Task StopRecording()
    {
        var audioUrl = await JSRuntime.InvokeAsync<string>("stopRecording");
        await JSRuntime.InvokeVoidAsync("eval", $"document.getElementById('audioPlayer').src = '{audioUrl}'");
    }

    private async Task ComparePronunciation()
    {
        comparisonResult = "النطق متطابق بنسبة 80%";
    }

    public void Dispose()
    {
        _ = chatHub?.DisposeAsync();
    }
}
