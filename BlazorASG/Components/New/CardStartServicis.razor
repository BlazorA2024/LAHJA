                        <textarea @bind="@messageInput" class="message-input" placeholder="اكتب رسالتك هنا..." rows="4"></textarea>
                        <button class="btn send-btn" @onclick="SendMessage">إرسال</button>
                        <ul class="message-list">
                            @foreach (var msg in messages)
                            {
                                <li class="message-item">@msg</li>
                            }
                        </ul>
                    </section>

                    <!-- حوار مع الذكاء الاصطناعي -->
                    <section id="ai" class="tab-section">
                        <h4 class="section-title">حوار مع الذكاء الاصطناعي</h4>
                        <textarea @bind="@aiInput" class="ai-input" placeholder="تحدث مع الذكاء الاصطناعي..." rows="4"></textarea>
                        <button class="btn ai-btn" @onclick="InteractWithAI">إرسال</button>
